  "repository": {
    "type": "git",
    "url": "git@github.com:zsh-crazy/vite-plugin-filter-css-moudle.git"
  },
  "author": "crazy-code",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@rollup/pluginutils": "^4.2.1",
    "@types/node": "^18.7.19",
    "cross-env": "^7.0.3",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "postcss": "^8.4.16",
    "postcss-modules": "^5.0.0",
    "rollup": "^2.79.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-typescript2": "^0.34.0",
    "typescript": "^4.8.3",
    "vite": "^3.1.3"
  },
  "peerDependencies": {
    "vite": "^3.1.0"
  }
